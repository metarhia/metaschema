{
  "name": "metaschema",
  "version": "0.0.28",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Metadata Schema and Interface Definition Language (IDL)",
  "license": "MIT",
  "keywords": [
    "interface",
    "definition",
    "metarhia",
    "idl",
    "dsl",
    "schema",
    "metadata"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/metaschema"
  },
  "main": "./metaschema.js",
  "bin": {
    "metaschema": "./bin/cli.js"
  },
  "browser": {
    "./metaschema.js": "./dist/metaschema.js",
    "./dist/lib/generator.js": false,
    "./dist/lib/introspection.js": false,
    "./dist/lib/schema-fs-loader.js": false
  },
  "readmeFilename": "README.md",
  "scripts": {
    "test": "npm run lint && npm run docs && metatests test/",
    "lint": "eslint --ignore-path .gitignore .",
    "docs": "./bin/cli.js ./test/example.js",
    "build": "babel metaschema.js -d dist && babel lib -d dist/lib",
    "prepublish": "npm run -s build"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@metarhia/common": "^1.0.0",
    "metasync": "^0.3.31"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "eslint": "^5.9.0",
    "eslint-config-metarhia": "^6.0.0",
    "eslint-plugin-import": "^2.14.0",
    "metatests": "^0.3.0"
  }
}
