  "engines": {
    "node": "^14.18 || 16 || 18 || 19"
  },
  "dependencies": {
    "metautil": "^3.7.1",
    "metavm": "^1.2.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.2",
    "eslint": "^8.36.0",
    "eslint-config-metarhia": "^8.1.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "metatests": "^0.8.2",
    "prettier": "^2.8.4",
    "typescript": "^5.0.4"
  }
