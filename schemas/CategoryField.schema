{
  domain: { domain: 'Nomen' },
  category: { domain: 'Nomen' },
  required: { domain: 'Logical' },
  default: { domain: 'Nomen' },
  comment: { domain: 'Text' },

  Check: Validate(data => {
    const isDomain = !!data.domain;
    const isCategory = !!data.category;
    return isDomain ? !isCategory : isCategory;
  })
}
